<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="type_of_sample_tree_view" model="ir.ui.view">
        <field name="name">Type of sample Tree</field>
        <field name="model">hr.hospital.type.of.sample</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id='action_type_of_sample' model='ir.actions.act_window'> 
        <field name='name'>Type of sample</field>
        <field name='res_model'>hr.hospital.type.of.sample</field> 
        <field name='view_mode'>tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                This list will store information about all types of sample
            </p>
        </field>
    </record>
</odoo>